<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>


<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.bigtwo.controllers.HowToPlayController">
   <children>
      <MediaView fx:id="tutorial" fitHeight="322.5" fitWidth="575.0" layoutX="13.0" layoutY="14.0" />
      <HBox layoutX="204.0" layoutY="349.0" prefHeight="23.0" prefWidth="195.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handlePause" text="Pause" />
            <Button mnemonicParsing="false" onAction="#handlePlay" text="Play" />
            <Button mnemonicParsing="false" onAction="#handleQuit" text="Quit" />
         </children>
      </HBox>
   </children>
</AnchorPane>
